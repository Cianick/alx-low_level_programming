#!/bin/bash

# Step 4: Create static library
ar -rcs libmy.a 0-isupper.o 0-memset.o 0-strcat.o 1-isdigit.o 1-memcpy.o 1-strncat.o 100-atoi.o 2-strchr.o 2-strlen.o 2-strncpy.o 3-islower.o 3-puts.o 3-strcmp.o 3-strspn.o 4-isalpha.o 4-strpbrk.o 5-strstr.o 6-abs.o 9-strcpy.o _putchar.o

# Step 5: List the contents of the library
ar -t libmy.a

# Step 6: Display the symbols in the library
nm libmy.a

# Step 7: Create main.c file
cat > main.c <<EOF
#include "main.h"

int main(void)
{
    _puts("\"At the end of the day, my goal was to be the best hacker\"\n\t- Kevin Mitnick");
    return (0);
}
EOF

# Step 8: Compile and link the program
gcc -std=gnu89 main.c -L. -lmy -o quote

# Step 9: Execute the program
./quote

# Step 10: Commit and push changes
git add libmy.a
git commit -m "Add static library"
git push

git add main.h
git commit -m "Add main header file"
git push

git add README.md
git commit -m "Update README"
git push

